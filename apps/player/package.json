{
  "name": "@podlove/player",
  "version": "5.12.0",
  "description": "HTML5 Goodness for Podcasting",
  "homepage": "https://github.com/podlove/ui/tree/development/apps/player",
  "type": "module",
  "scripts": {
    "serve": "vite serve",
    "build": "vite build",
    "postpublish": "npm run clean",
    "integration:ci": "cypress run --browser chrome",
    "integration:dev": "cypress open --browser chrome"
  },
  "keywords": [],
  "author": "Alexander Heimbuch <github@heimbu.ch>",
  "license": "MIT",
  "dependencies": {
    "@podlove/button-actions": "workspace:*",
    "@podlove/components": "workspace:*",
    "@podlove/player-actions": "workspace:*",
    "@podlove/player-config": "workspace:*",
    "@podlove/player-sagas": "workspace:*",
    "@podlove/player-state": "workspace:*",
    "@podlove/types": "workspace:*",
    "@podlove/utils": "workspace:*",
    "core-js": "3.15.2",
    "date-fns": "2.16.1",
    "ramda": "0.29.0",
    "redux": "4.2.0",
    "redux-vuex": "3.0.0",
    "vue": "3.2.41",
    "vue-i18n": "9.2.2",
    "farbraum": "0.1.4",
    "tailwindcss": "3.2.4",
    "reselect": "4.1.8"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "3.2.0",
    "cypress-vite": "1.4.2",
    "vite": "4.3.5",
    "vite-plugin-dynamic-publicpath": "1.1.2",
    "http-server": "14.1.1",
    "typescript": "5.0.4",
    "cypress": "13.1.0",
    "@types/node": "20.8.4"
  },
  "exports": {
    ".": {
      "import": "./src/main.ts",
      "types": "./src/main.ts"
    },
    "./styles": {
      "import": "./src/styles.css",
      "types": "./src/styles.css"
    }
  },
  "publishConfig": {
    "exports": {
      ".": {
        "import": "./dist/player.mjs",
        "types": "./dist/player.d.ts"
      }
    }
  }
}
