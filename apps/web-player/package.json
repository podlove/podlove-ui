{
  "name": "@podlove/web-player",
  "version": "5.12.0",
  "description": "Embedded version of Podlove Player",
  "homepage": "https://github.com/podlove/ui/tree/development/apps/web-player",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "integration:build": "npm run clean && npm run build:dependencies && webpack --config .build/webpack.config.dev.js",
    "integration:server": "http-server -p 9000 dist/ > http.log 2>&1 &",
    "integration:ci": "cypress run --browser chrome",
    "integration:dev": "cypress open --browser chrome",
    "build": "vite build",
    "serve": "vite serve"
  },
  "keywords": [],
  "author": "Alexander Heimbuch <github@heimbu.ch>",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@podlove/button-actions": "workspace:*",
    "@podlove/player": "workspace:*",
    "@podlove/player-actions": "workspace:*",
    "@podlove/player-config": "workspace:*",
    "@podlove/player-state": "workspace:*",
    "@podlove/subscribe-button": "workspace:*",
    "@podlove/types": "workspace:*",
    "@podlove/utils": "workspace:*",
    "color": "4.2.3",
    "hashcode": "1.0.3",
    "iframe-resizer": "4.3.2",
    "ramda": "0.29.0"
  },
  "devDependencies": {
    "@podlove/build": "workspace:*",
    "cypress": "13.1.0",
    "http-server": "0.11.1",
    "webpack": "5.45.1",
    "webpack-dev-server": "3.11.2",
    "@types/ramda": "0.29.3",
    "redux": "4.2.1",
    "vite": "4.3.9",
    "@vitejs/plugin-vue": "4.2.3",
    "vite-plugin-dynamic-publicpath": "1.1.2"
  },
  "exports": {
    ".": {
      "import": "./src/main.ts",
      "types": "./src/main.ts"
    }
  }
}
