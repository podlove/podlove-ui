  "license": "MIT",
  "dependencies": {
    "@podlove/button-actions": "workspace:*",
    "@podlove/button-state": "workspace:*",
    "@podlove/clients": "workspace:*",
    "@podlove/components": "workspace:*",
    "@podlove/utils": "workspace:*",
    "ramda": "0.29.0",
    "redux": "4.2.1",
    "redux-saga": "1.2.3",
    "redux-vuex": "3.0.0",
    "vue": "3.2.41",
    "vue-i18n": "9.2.2",
    "tailwindcss": "3.2.4",
    "reselect": "4.1.8",
    "detect-browser": "5.3.0"
  },
  "devDependencies": {
    "vite-plugin-dynamic-publicpath": "1.1.2",
    "vite-plugin-environment": "1.1.3",
    "@fullhuman/postcss-purgecss": "5.0.0",
    "http-server": "14.1.1",
    "npm-run-all": "4.1.5",
