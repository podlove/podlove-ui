{
  "name": "@podlove/components",
  "version": "5.12.0",
  "description": "Reusable Podlove UI Components",
  "homepage": "https://github.com/podlove/ui/tree/development/packages/components",
  "scripts": {
    "clean": "rm -rf dist/*",
    "build": "vite build --config ./vite.config.js",
    "test": "vitest run",
    "test:dev": "vitest"
  },
  "keywords": [],
  "author": "Alexander Heimbuch <github@heimbu.ch>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/podlove-ui-components.umd.js",
  "module": "./dist/podlove-ui-components.es.js",
  "exports": {
    ".": {
      "import": "./dist/podlove-ui-components.es.js",
      "require": "./dist/podlove-ui-components.umd.js"
    }
  },
  "dependencies": {
    "@podlove/player-actions": "^5.12.0",
    "@podlove/utils": "^5.12.0",
    "rangetouch": "2.0.1",
    "v-tooltip": "4.0.0-beta.17",
    "vue": "3.2.41",
    "tailwindcss": "3.2.4",
    "ramda": "0.29.0"
  },
  "peerDependencies": {
    "vue": "3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "3.2.0",
    "@types/ramda": "0.28.22",
    "vite": "4.2.1",
    "vitest": "0.30.0",
    "@vue/test-utils": "2.3.2",
    "happy-dom": "9.1.9",
    "typescript": "5.0.4"
  }
}
