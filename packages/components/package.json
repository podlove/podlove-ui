{
  "name": "@podlove/components",
  "version": "4.5.0",
  "description": "Reusable Podlove UI Components",
  "homepage": "https://github.com/podlove/ui/tree/development/packages/components",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist publish",
    "dev": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build": "npm run clean && webpack --config .build/webpack.config.js",
    "postinstall": "npm run build",
    "publish:prepare": "npm run clean && npm run build && mkdir -p publish && cp -R dist/ publish/ && cp package.json publish/"
  },
  "keywords": [],
  "author": "Alexander Heimbuch <github@heimbu.ch>",
  "license": "MIT",
  "dependencies": {
    "@podlove/player-actions": "^4.5.0",
    "@podlove/utils": "^4.5.0",
    "lodash": "4.17.11",
    "normalize.css": "8.0.1",
    "v-tooltip": "2.0.0-rc.33",
    "vue": "2.6.10"
  },
  "devDependencies": {
    "@podlove/build": "^4.5.0",
    "@storybook/addon-actions": "5.0.5",
    "@storybook/addon-centered": "5.0.5",
    "@storybook/addon-knobs": "5.0.5",
    "@storybook/addon-options": "5.0.5",
    "@storybook/addons": "5.0.5",
    "@storybook/vue": "5.0.5",
    "terser": "3.14.1",
    "webpack": "4.26.0",
    "webpack-dev-server": "3.1.11"
  }
}
