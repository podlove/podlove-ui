{
  "name": "@podlove/components",
  "version": "5.12.0",
  "description": "Reusable Podlove UI Components",
  "homepage": "https://github.com/podlove/ui/tree/development/packages/components",
  "scripts": {
    "clean": "rm -rf dist/*",
    "build": "vite build --config ./vite.config.js",
    "test": "vitest run",
    "test:dev": "vitest"
  },
  "keywords": [],
  "author": "Alexander Heimbuch <github@heimbu.ch>",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/components/main.ts",
      "types": "./src/components/main.ts"
    }
  },
  "publishConfig": {
    "exports": {
      ".": {
        "import": "./dist/podlove-ui-components.mjs",
        "types": "./dist/podlove-ui-components.d.ts"
      }
    }
  },
  "dependencies": {
    "@podlove/player-actions": "workspace:*",
    "@podlove/utils": "workspace:*",
    "rangetouch": "2.0.1",
    "floating-vue": "2.0.0-beta.20",
    "@floating-ui/dom": "1.2.7",
    "vue": "3.2.41",
    "tailwindcss": "3.2.4",
    "ramda": "0.29.0"
  },
  "peerDependencies": {
    "vue": "3"
  },
  "devDependencies": {
    "@podlove/types": "workspace:*",
    "@vitejs/plugin-vue": "4.2.1",
    "@types/ramda": "0.29.1",
    "vite": "4.3.5",
    "vitest": "0.30.0",
    "@vue/test-utils": "2.3.2",
    "happy-dom": "9.1.9",
    "typescript": "5.0.4",
    "vite-plugin-dts": "2.3.0",
    "vite-plugin-css-injected-by-js": "3.1.0"
  }
}
