      "import": "./src/clients.ts",
      "types": "./src/clients.ts"
    },
    "./config": {
      "import": "./src/config.ts",
      "types": "./src/config.ts"
    },
    "./feed": {
      "import": "./src/feed.ts",
      "types": "./src/feed.ts"
    },
    "./finish": {
      "import": "./src/finish.ts",
      "types": "./src/finish.ts"
    },
    "./runtime": {
      "import": "./src/runtime.ts",
      "types": "./src/runtime.ts"
    },
    "./theme": {
      "import": "./src/theme.ts",
      "types": "./src/theme.ts"
    },
    "./view": {
      "import": "./src/view.ts",
      "types": "./src/view.ts"
    }
  },
  "publishConfig": {
    "exports": {
      "./clients": {
        "import": "./dist/clients.mjs",
        "types": "./dist/clients.d.ts"
      },
      "./config": {
        "import": "./dist/config.mjs",
        "types": "./dist/config.d.ts"
      },
      "./feed": {
        "import": "./dist/feed.mjs",
        "types": "./dist/feed.d.ts"
      },
      "./finish": {
        "import": "./dist/finish.mjs",
        "types": "./dist/finish.d.ts"
      },
      "./runtime": {
        "import": "./dist/runtime.mjs",
        "types": "./dist/runtime.d.ts"
      },
      "./theme": {
        "import": "./dist/theme.mjs",
        "types": "./dist/theme.d.ts"
      },
      "./view": {
        "import": "./dist/view.mjs",
        "types": "./dist/view.d.ts"
